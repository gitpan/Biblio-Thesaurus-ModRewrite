  t1 $r $a      ==> sub{ warn( "isa reclexia para $a\n"); }
  t1 isa $a     ==> sub{ warn( "isa reclexia para $a\n"); }
  $a isa $a     ==> sub{ warn( "isa reclexia para $a\n"); }

